float3 main(v2p_volume I, float4 pos2d : SV_Position) : SV_Target
{
	float2 tc = I.tc.xy / I.tc.ww;
	float3 gi = rsm_accum_hashed(tc, pos2d);
	float flux = 1;//Ldynamic_pos.w;
	return gi * flux;
}